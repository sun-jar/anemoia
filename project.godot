; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Anemoia"
config/version="1.0.0.0"
run/main_scene="res://scenes/user_interface/SplashScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Anemoia"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://scenes/AudioManager.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
GameManager="*res://scenes/GameManager.tscn"
Globals="*res://scripts/Globals.gd"

[dialogic]

directories/dch_directory={
"unknown": "res://dialogue/unknown.dch"
}
directories/dtl_directory={
"guide": "res://dialogue/guide.dtl",
"timeline": "res://dialogue/timeline.dtl"
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.05
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
save/autosave_delay=60.0
save/encryption_on_exports_only=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
audio/max_channels=4.0
text/autopauses={}
glossary/default_case_sensitive=true
variables={}
layout/style_list=["res://dialogue/dialogue_style.tres"]
layout/default_style="res://dialogue/dialogue_style.tres"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

